<?xml version="1.0" encoding="utf-8"?>
<project>
	
	<meta title="Testris" package="com.jcigaems.testris" version="0.0.1" company="jCi gaems" />
	<app main="Main" path="Export" file="testris" title="Testris" />
	<set name="ANDROID_INSTALL_LOCATION" value="auto" />

	<window width="480" height="320" orientation="landscape"  fullscreen="false" fps="30" background="0x000000"  />
	<window width="480" height="320" orientation="landscape"  fullscreen="true" fps="30" background="0x000000"  if="mobile" />
	<source path="Source" />
	
	<haxelib name="openfl" />
	<haxelib name="openfl-html5-dom" if="html5" />
	
	<assets path="Assets" rename="assets" exclude="openfl.svg" include="*.*" />
	<icon path="Assets/openfl.svg" />
	
</project>
